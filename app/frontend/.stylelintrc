{
  "extends": "stylelint-config-recommended-vue",
  "rules": {
    "no-empty-source": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "tailwind",
          "apply",
          "variants",
          "responsive",
          "screen"
        ]
      }
    ],
    "no-descending-specificity": null
  }
}
